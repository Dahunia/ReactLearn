{
    "expo": {
        "name": "Self Blog",
        "slug": "rn-self-posts",
        "platforms": ["ios", "android"],
        "version": "1.0.0",
        "icon": "./assets/rn-icon.png",
        "splash": {
            "image": "./assets/rn-splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#242832"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.iadaria",
            "buildNumber": "1.0.0"
        },
        "android": {
            "package": "com.iadaria",
            "versionCode": 1,
            "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
        }
    }
}
